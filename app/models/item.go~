type Item struct {
  itemID           string    `json:"userID"`
  itemSupplier      string    `json:"userCompany"`
  itemStockQuantity int32    `json:"userDiscountRate"`
  itemBasePrice string    `json:"guest_staring_role"`
  itemTags string    `json:"guest_staring_role"`
}